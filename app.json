{
  "expo": {
    "name": "blood-bank",
    "slug": "blood-bank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/android/play_store_512.png",
    "scheme": "bloodbank",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/android/play_store_512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bloodbank.app",
      "buildNumber": "1.0.0",
      "icon": "./assets/images/ios/AppIcon~ios-marketing.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android/play_store_512.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bloodbank.app",
      "versionCode": 1
    },
    "web": {
      "output": "server",
      "bundler": "metro",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/android/play_store_512.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "86eb4501-a1e3-4678-a4ce-eb310ad5313b"
      }
    }
  }
}
